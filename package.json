{
  "name": "kaleab",
  "description": "Kaleab S. Melkie's official website",
  "version": "2.3.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "^12"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "lint": "prettier public/ src/ *.{ts,json,md}",
    "format": "prettier --write public/ src/ *.{ts,json,md}"
  },
  "dependencies": {
    "next": "9.5.3",
    "next-compose-plugins": "2.2.0",
    "next-manifest": "3.0.1",
    "next-offline": "5.0.2",
    "next-seo": "4.7.3",
    "node-sass": "4.14.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-icons": "3.11.0"
  },
  "devDependencies": {
    "@types/node": "^12",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "husky": "4.3.0",
    "prettier": "2.1.1",
    "typescript": "4.0.2",
    "webpack": "5.0.0-beta.30"
  },
  "prettier": {
    "endOfLine": "lf",
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn build && git update-index --again"
    }
  }
}
