{
  "name": "kaleab",
  "description": "Kaleab S. Melkie's official website",
  "version": "2.10.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE=true NODE_ENV=production next build",
    "build": "cross-env NODE_ENV=production next build",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "format": "prettier --write pages/ public/ src/ *.{ts,json,md}",
    "lint": "prettier pages/ public/ src/ *.{ts,json,md}",
    "start": "cross-env NODE_ENV=production next start",
    "pm2": "pm2",
    "pm2:start": "pm2 start pm2.json"
  },
  "resolutions": {
    "**/webpack": "5.9.0"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "3.0.0",
    "@next/bundle-analyzer": "10.0.3",
    "@types/node": "^14",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "compression-webpack-plugin": "7.0.0",
    "cross-env": "7.0.3",
    "husky": "4.3.0",
    "next": "10.0.3",
    "next-compose-plugins": "2.2.1",
    "next-manifest": "3.0.1",
    "next-offline": "5.0.3",
    "next-plugin-preact": "3.0.3",
    "next-secure-headers": "2.0.0",
    "next-seo": "4.17.0",
    "node-sass": "5.0.0",
    "pm2": "4.5.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "6.7.0",
    "preact": "10.5.7",
    "preact-render-to-string": "5.1.12",
    "prettier": "2.2.1",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-icons": "4.1.0",
    "typescript": "4.1.2",
    "webpack": "5.9.0"
  },
  "prettier": {
    "endOfLine": "lf",
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn build && yarn format && git update-index --again"
    }
  }
}
