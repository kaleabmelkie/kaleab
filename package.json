{
  "name": "kaleab",
  "description": "Kaleab S. Melkie's official website",
  "version": "2.10.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE=true NODE_ENV=production next build",
    "build": "cross-env NODE_ENV=production next build",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "format": "prettier --write pages/ public/ src/ *.{ts,json,md}",
    "lint": "prettier pages/ public/ src/ *.{ts,json,md}",
    "start": "cross-env NODE_ENV=production next start"
  },
  "dependencies": {
    "@next/bundle-analyzer": "10.0.1",
    "next": "10.0.1",
    "next-compose-plugins": "2.2.1",
    "next-manifest": "3.0.1",
    "next-offline": "5.0.3",
    "next-plugin-preact": "3.0.3",
    "next-secure-headers": "2.0.0",
    "next-seo": "4.17.0",
    "node-sass": "4.14.1",
    "preact": "10.5.7",
    "preact-render-to-string": "5.1.11",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-icons": "3.11.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "3.0.0",
    "@types/node": "^14",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "compression-webpack-plugin": "6.1.1",
    "cross-env": "7.0.2",
    "husky": "4.3.0",
    "postcss-flexbugs-fixes": "5.0.1",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.1.2",
    "typescript": "4.0.5"
  },
  "prettier": {
    "endOfLine": "lf",
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn build && yarn format && git update-index --again"
    }
  }
}
